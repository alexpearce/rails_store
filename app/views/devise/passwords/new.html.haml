= title 'Forgot Your Password?'
= form_for resource, :as => resource_name, :url => password_path(resource_name), :html => { :method => :post, :class => 'admin' } do |f|
  = devise_error_messages!
  %div.field
    = f.label :email
    = f.text_field :email
  %div.actions
    = f.submit "Send"

= render :partial => "devise/shared/links"
